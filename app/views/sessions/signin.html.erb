<%= form_tag signin_path, method: :post do |f| %>
    <p>
        <%= label_tag :name %>
        <%= collection_select(:user, :name, User.all, :id, :name) %>
    </p>
    <p>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </p>
    <%= submit_tag "Sign In" %>
<% end %>